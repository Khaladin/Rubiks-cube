{
  "name": "rubiks-cube",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^6.0.3",
    "jquery": "^3.4.1",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:nowatch": "cross-env CI=true react-scripts test --passWithNoTests",
    "pretty": "Prettier --write --tab-width 2 \"src/**/*.js\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch pretty"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^3.0.9"
  }
}
